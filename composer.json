{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^3.2",
        "doctrine/doctrine-migrations-bundle": "^4.0",
        "doctrine/orm": "^3.6",
        "egulias/email-validator": "^4.0",
        "fakerphp/faker": "^1.24",
        "gesdinet/jwt-refresh-token-bundle": "^1.5",
        "lexik/jwt-authentication-bundle": "^3.2",
        "nelmio/api-doc-bundle": "^5.9",
        "nelmio/cors-bundle": "^2.6",
        "phpdocumentor/reflection-docblock": "^5.6.6",
        "phpdocumentor/type-resolver": "^1.12",
        "phpstan/phpdoc-parser": "^1.33",
        "symfony/asset": "7.4.*",
        "symfony/cache": "7.4.*",
        "symfony/console": "7.4.*",
        "symfony/dependency-injection": "7.4.*",
        "symfony/dotenv": "7.4.*",
        "symfony/flex": "^2",
        "symfony/framework-bundle": "7.4.*",
        "symfony/http-client": "7.4.*",
        "symfony/http-foundation": "7.4.*",
        "symfony/http-kernel": "7.4.*",
        "symfony/monolog-bundle": "^4.0",
        "symfony/password-hasher": "7.4.*",
        "symfony/property-access": "7.4.*",
        "symfony/property-info": "7.4.*",
        "symfony/runtime": "7.4.*",
        "symfony/security-bundle": "7.4.*",
        "symfony/security-csrf": "7.4.*",
        "symfony/serializer": "7.4.*",
        "symfony/twig-bundle": "7.4.*",
        "symfony/uid": "7.4.*",
        "symfony/validator": "7.4.*",
        "symfony/yaml": "7.4.*",
        "web-token/jwt-framework": "^4.1"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^4.3",
        "friendsofphp/php-cs-fixer": "^3.93",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.35",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "rector/rector": "^2.3",
        "symfony/maker-bundle": "^1.65"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "rector": "php vendor/bin/rector process --config=./rector.php --dry-run --memory-limit=16G",
        "rector:fix": "php vendor/bin/rector process --config=./rector.php --memory-limit=16G",
        "cs": "php vendor/bin/php-cs-fixer fix --config=./php-cs-fixer.php --dry-run",
        "cs:fix": "php vendor/bin/php-cs-fixer fix --config=./php-cs-fixer.php",
        "phpstan": "php vendor/bin/phpstan analyse -c phpstan.neon.dist --memory-limit=16G"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    }
}
